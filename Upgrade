      Customer@1000 : Record 18;
    BEGIN
      IF NOT Customer.FINDSET THEN
        EXIT;

      Customer.SETAUTOCALCFIELDS(Picture);

      REPEAT
        ConvertBlobToMediaSetCustomer(Customer);
      UNTIL Customer.NEXT = 0;
    END;

